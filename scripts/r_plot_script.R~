mydata = read.csv("/home/eddie/Documents/dev/projects/c/test_code/cache_line_test/data/affinity_maxpri.csv")
vals<-c(mydata$X)
plot(vals, type="l", xlab="Sample #", ylab="Time (ticks)")
title("CPU Affinity, MAX_PRIO")

